  "swagger": "2.0",
  "info": {
    "description": "This documentation describes the RTME/CFG API.",
    "title": "V1 API.",
    "version": "1.0.0"
  },
  "basePath": "/api/v1",
  "paths": {
    "/app/config": {
      "post": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "app"
        ],
        "summary": "Setup app config",
        "operationId": "appConfig",
        "parameters": [
          {
            "type": "string",
            "description": "db type",
            "name": "DBTYPE",
            "in": "body",
            "required": true
          },
          {
            "type": "string",
            "description": "db rtme url",
            "name": "DBRTMEURL",
            "in": "body",
            "required": true
          },
          {
            "type": "string",
            "description": "db cfg url",
            "name": "DBCFGURL",
            "in": "body",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    },
    "/app/status": {
      "get": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "app"
        ],
        "summary": "Get app status",
        "operationId": "appStatus",
        "responses": {
          "200": {
            "description": "status response"
          }
        }
      }
    },
    "/cfg/groups": {
      "get": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "config"
        ],
        "summary": "Get group name list",
        "operationId": "cfgGroups",
        "responses": {
          "200": {
            "description": "Groups names"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    },
    "/cfg/groups/{name}/users": {
      "get": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "config"
        ],
        "summary": "Get group agent list",
        "operationId": "cfgGroups",
        "parameters": [
          {
            "type": "string",
            "description": "name of group",
            "name": "name",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Agent dbid list"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    },
    "/cfg/places/{dbid}": {
      "get": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "config"
        ],
        "summary": "Get info on a place",
        "operationId": "cfgPlace",
        "parameters": [
          {
            "type": "string",
            "description": "id of place",
            "name": "dbid",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Place info"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    },
    "/rtme/{date}/login": {
      "get": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "rtme"
        ],
        "summary": "Export rtme info of login.",
        "operationId": "rtmeLogin",
        "parameters": [
          {
            "type": "string",
            "description": "date",
            "name": "date",
            "in": "path",
            "required": true
          },
          {
            "type": "string",
            "description": "filter by group/instance name",
            "name": "group",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Logins info",
            "$ref": "#/responses/RTMELoginResponse"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    },
    "/rtme/{date}/status": {
      "get": {
        "produces": [
          "application/json"
        ],
        "tags": [
          "rtme"
        ],
        "summary": "Export rtme info of status.",
        "operationId": "rtmeStatus",
        "parameters": [
          {
            "type": "string",
            "description": "date",
            "name": "date",
            "in": "path",
            "required": true
          },
          {
            "type": "string",
            "description": "filter by group/instance name",
            "name": "group",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Status info",
            "$ref": "#/responses/RTMEStatusResponse"
          },
          "400": {
            "$ref": "#/responses/BadRequest"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    }
